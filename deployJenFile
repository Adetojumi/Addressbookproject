pipeline {
  agent any
  tools {
    maven 'mymaven' 
  }
  stages {
    stage ('Build') {
      steps {
        sh 'mvn clean package'
      }
    }
    stage ('Deploy') {
      steps {
        sshagent(['jenkins_techports']) {
          sh "scp -o StrictHostKeyChecking=no target/*.war techports@54.161.153.33:8080:/home/techports/tomcat/webapps" 
}
      }
    }
  }
}
